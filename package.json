{
  "name": "solid-typescript-webpack",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "coverage": "jest --coverage",
    "dev": "webpack-dev-server --hot --port 3000 --mode development",
    "lint": "eslint --ext .ts,.tsx src/",
    "open:coverage": "npm run coverage && open-cli ./coverage/lcov-report/index.html",
    "start": "npm run dev -- --open",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "solid-js": "^0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/dom": "^7.16.1",
    "@testing-library/jest-dom": "^5.10.1",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.13",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/terser-webpack-plugin": "^3.0.0",
    "@types/webpack": "^4.41.17",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-loader": "^8.1.0",
    "babel-preset-solid": "^0.18.7",
    "css-loader": "^3.6.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.3.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "open-cli": "^6.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "terser-webpack-plugin": "^3.0.5",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "roots": [
      "src"
    ],
    "preset": "ts-jest",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "assets",
      ".css.d.ts"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
